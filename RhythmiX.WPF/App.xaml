<Application x:Class="RhythmiX.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RhythmiX.WPF">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/UIColors.xaml" />
                <ResourceDictionary Source="Styles/ButtonsStyle.xaml" />
                <ResourceDictionary Source="Styles/CustomInputFieldStyle.xaml" />
                <ResourceDictionary Source="Styles/LoginElementsStyle.xaml" />
                <ResourceDictionary Source="Styles/SignupElementsStyle.xaml" />
                <ResourceDictionary Source="Styles/TrackLayoutStyle.xaml" />
                <ResourceDictionary Source="Styles/ViewsCommonStyle.xaml" />
                <ResourceDictionary Source="Styles/PlaylistLayoutStyle.xaml" />
                <ResourceDictionary Source="Styles/AlbumLayoutStyle.xaml" />
                <ResourceDictionary Source="Styles/ArtistLayoutStyle.xaml" />
                <ResourceDictionary Source="Styles/FontFamilies.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
